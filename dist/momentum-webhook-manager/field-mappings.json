  "enhancement_rules": {
    "numeric_id_pattern": "^[1-9]\\d{0,8}$",
    "max_id_value": 999999999,
    "default_enhancement_format": "{field_name}: {id}",
    "context_separator": ".",
    "array_index_format": "[{index}]"
  },
